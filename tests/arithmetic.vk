'tests/test' include

'abs: must push the absolute value of the top element' [
  -1 abs
  1 must-equal

  1 abs
  1 must-equal
] test

'add: must add the top two elements' [
  5 2 add
  7 must-equal
] test

'dec: must subtract 1 from top element' [
  5 dec
  4 must-equal
] test

'div: must divide the second element by the top' [
  12 3 div
  4 must-equal
] test

'fact: must calculate the factorial of the top element' [
  5 fact
  120 must-equal
] test

'inc: must add 1 to top element' [
  5 inc
  6 must-equal
] test

'mult: must multiply top two elements' [
  5 3 mult
  15 must-equal
] test

'neg: must negate top element' [
  5 neg
  -5 must-equal

  -10 neg
  10 must-equal
] test

'prod: must multiply all elements on the stack together' [
  5 7 9 11 prod
  3465 must-equal
] test

'sub: must subtract top element from the second' [
  5 2 sub
  3 must-equal
] test

'sum: must sum all elements on the stack' [
  5 7 9 11 sum
  32 must-equal
] test
