'tests/test' include

'dup: must duplicate value at the top of the stack' [
  'a' dup
  'a' must-equal
] test

'pop: must remove value at the top of the stack' [
  'a' pop
  must-be-empty
] test

'size: must push the size of the stack' [
  1 2 3 4 size
  4 must-equal
] test

'drop: must remove all items on the stack' [
  1 2 3 4 drop
  must-be-empty
] test

'compose: must compose two blocks into one' [
  :inc :dec compose
  [ inc dec ] must-equal
] test

'stack: must call function for each pair of elements' [
  1 2 3 4 0 :inc stack
  4 must-equal
] test

'swap: must swap top two elements' [
  1 2 swap
  1 must-equal
  2 must-equal
] test

'swapp: must swap second and third elements' [
  1 2 3 swapp
  3 must-equal
  1 must-equal
  2 must-equal
] test
